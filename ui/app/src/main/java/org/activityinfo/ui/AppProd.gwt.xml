<module rename-to="App3">

    <inherits name='org.activityinfo.ui.App'/>

    <!-- Minimize CSS names -->
    <set-configuration-property name="CssResource.style" value="obf"/>

    <!-- Turn off logging -->
    <set-property name="gwt.logging.enabled" value="FALSE"/>

    <!-- Limit to supported browsers -->
    <set-property name="gxt.user.agent" value="safari5,chrome,ie9,ie10,ie11,gecko1_9"/>

    <collapse-property name="user.agent" values="ie9,ie10" />
    <collapse-property name="user.agent" values="safari,gecko1_8" />

</module>
